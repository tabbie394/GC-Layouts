    position: relative;

    background:
        radial-gradient(ellipse at 30% 25%,
            rgba(255, 255, 255, 0.55),
            rgba(255, 235, 250, 0.35) 40%,
            rgba(243, 200, 240, 0.22) 65%,
            rgba(220, 113, 228, 0.12) 85%,
            rgba(220, 113, 228, 0.06) 100%);

    background-size: 180% 180%;
    background-position: 0% 0%;

    backdrop-filter: blur(7px);
    -webkit-backdrop-filter: blur(7px);

    border-radius: 18px;

    box-shadow:
        0 0 12px rgba(255, 255, 255, 0.35),
        0 0 30px rgba(220, 113, 228, 0.18),
        inset 0 0 14px rgba(255, 255, 255, 0.28);

    animation: cloud-shift-a 18s ease-in-out infinite;
}

